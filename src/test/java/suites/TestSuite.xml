<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite">

    <listeners>
        <listener class-name="listeners.RetryListener"></listener>
        <listener class-name="listeners.ScreenshotListener"></listener>
    </listeners>

    <parameter name="browser" value="Chrome"></parameter>

    <test name="All Tests">
        <classes>
            <class name="tests.FailedLoginTest"></class>
            <class name="tests.PretragaFilteriTest"></class>
            <class name="tests.TenisPatikeTest"></class>
            <class name="tests.CategoryTest"></class>
            <class name="tests.AddRandomItem"></class>
        </classes>
    </test>
</suite>